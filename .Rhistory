give_msg_time <- function(time.1) {
time.2 <- Sys.time()
# Time spent
alltime <- round(as.numeric(difftime(time.2, time.1, units = "mins")), 2)
t.sec <- round((alltime - trunc(alltime)) * 60)
msg <- paste("Completed in ", trunc(alltime), "min", t.sec, "sec\n")
return(msg)
}
